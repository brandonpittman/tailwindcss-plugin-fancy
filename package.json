{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --target node",
    "build": "tsdx build --target node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --target node",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "tailwindcss-plugin-fancy",
  "author": "Brandon Pittman",
  "module": "dist/twfancy.esm.js",
  "size-limit": [
    {
      "path": "dist/twfancy.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/twfancy.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "autoprefixer": "^10.2.5",
    "husky": "^5.2.0",
    "postcss": "^8.2.8",
    "postcss-selector-parser": "^6.0.4",
    "size-limit": "^4.10.1",
    "tailwindcss": "^2.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
