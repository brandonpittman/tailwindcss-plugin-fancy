{
  "name": "tailwindcss-plugin-fancy",
  "version": "1.2.3",
  "description": "A collection of other Tailwind CSS plugins all rolled up for your convenience.",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandonpittman/tailwindcss-plugin-fancy.git"
  },
  "files": [
    "dist",
    "index.js",
    "packages"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "clean": "del dist",
    "build": "npm run clean && ncc build index.js -o dist -m && cpy packages/heroicons/icons/**/*.svg dist/icons",
    "test": "xo --fix"
  },
  "dependencies": {},
  "keywords": [
    "tailwindcss"
  ],
  "author": "Brandon Pittman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonpittman/tailwindcss-plugin-fancy/issues"
  },
  "homepage": "https://github.com/brandonpittman/tailwindcss-plugin-fancy#readme",
  "devDependencies": {
    "@tailwindcss/ui": "latest",
    "@zeit/ncc": "^0.21.1",
    "cpy-cli": "^3.1.0",
    "del-cli": "^3.0.0",
    "tailwindcss": "^1.2.0",
    "xo": "^0.27.2"
  }
}
